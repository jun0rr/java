<log name="us.pserver">
  <class>us.pserver.log.impl.SimpleLog</class>
  <levels>
    <all>true</all>
    <debug>false</debug>
  </levels>
  <output name="stdout">
    <class>us.pserver.log.output.StandardOutput</class>
    <levels>
      <all>true</all>
      <debug>false</debug>
      <error>false</error>
    </levels>
  </output>
  <output name="stderr">
    <class>us.pserver.log.output.StandardErrorOutput</class>
    <levels>
      <error>true</error>
    </levels>
  </output>
  <output name="stdout">
    <class>us.pserver.log.output.NullOutput</class>
    <levels>
      <debug>true</debug>
    </levels>
  </output>
</log>
